{
  "SYNC_SETTINGS"  : {
        "isSync"   : true,
        "level"    : "1",
        "db"       : "block_cluster",
        "table"    : "cluster_info",
        "logfile"  : "sync_mysql2es.log",
        "batch"    : 5,
        "debug"    : "Y"
   },

   "MYSQL_SETTINGS" : {
        "host"  : "192.168.3.5",
        "port"  : 3306,
        "user"  : "root",
        "passwd": "Dev21@block2022"
    },

    "ES_SETTINGS": {
        "host": "192.168.3.43",
        "port": "9200",
        "schema": "http",
        "user": "elastic",
        "passwd": "21block@2022",
        "index": "21block_cluster_info2",
        "batch": 3,
        "mapping": {
            "mappings": {
                "dynamic_templates": [
                    {
                        "message_full": {
                            "match": "message_full",
                            "mapping": {
                                "fields": {
                                    "keyword": {
                                        "ignore_above": 2048,
                                        "type": "keyword"
                                    }
                                },
                                "type": "text"
                            }
                        }
                    },
                    {
                        "message": {
                            "match": "message",
                            "mapping": {
                                "type": "text"
                            }
                        }
                    },
                    {
                        "strings": {
                            "match_mapping_type": "string",
                            "mapping": {
                                "type": "keyword"
                            }
                        }
                    }
                ],
                "properties": {
                    "area_code": {
                        "type": "keyword"
                    },
                    "audit_status": {
                        "type": "integer"
                    },
                    "begin_time": {
                        "type": "date"
                    },
                    "business_id": {
                        "type": "integer"
                    },
                    "business_name": {
                        "type": "text",
                        "analyzer": "ik_max_word"
                    },
                    "category_id": {
                        "type": "long"
                    },
                    "charge_mode": {
                        "type": "integer"
                    },
                    "city_code": {
                        "type": "keyword"
                    },
                    "cluster_desc": {
                        "type": "text",
                        "analyzer": "ik_max_word"
                    },
                    "cluster_id": {
                        "type": "keyword"
                    },
                    "cluster_photo_urls": {
                        "type": "keyword"
                    },
                    "cluster_status": {
                        "type": "integer"
                    },
                    "cluster_type": {
                        "type": "integer"
                    },
                    "create_time": {
                        "type": "date"
                    },
                    "current_number": {
                        "type": "integer"
                    },
                    "deadline_time": {
                        "type": "date"
                    },
                    "finish_time": {
                        "type": "date"
                    },
                    "fixed_amount": {
                        "type": "long"
                    },
                    "found_user_id": {
                        "type": "long"
                    },
                    "ip_address": {
                        "type": "keyword"
                    },
                    "locat_amount": {
                        "type": "long"
                    },
                    "locate_name": {
                        "type": "text",
                        "analyzer": "standard"
                    },
                    "location": {
                        "type": "geo_point"
                    },
                    "max_number": {
                        "type": "integer"
                    },
                    "min_number": {
                        "type": "integer"
                    },
                    "province_code": {
                        "type": "keyword"
                    },
                    "remark": {
                        "type": "text",
                        "analyzer": "standard"
                    },
                    "ticket_amount": {
                        "type": "long"
                    },
                    "title": {
                        "type": "text",
                        "analyzer": "ik_max_word"
                    },
                    "begin_hour_minute": {
                        "type": "keyword"
                    },
                    "update_time": {
                        "type": "date"
                    }
                }
            }
        }
    }
}